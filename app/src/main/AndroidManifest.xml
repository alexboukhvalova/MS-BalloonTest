<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.cmsc436.bubbletest">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
        android:name=".PopActivity"
        android:exported="true" >
            <intent-filter>
                
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="edu.umd.cmsc436.pop.action.HELP"/>
                <action android:name="edu.umd.cmsc436.pop.action.TRIAL"/>
                <action android:name="edu.umd.cmsc436.pop.action.PRACTICE"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

    </application>

</manifest>
